<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#0092C3" />
            <corners android:radius="4dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <solid android:color="#FF00FF00" />
        </shape>
    </item>

    <item
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <shape android:shape="oval">
            <stroke
                android:width="4dp"
                android:color="#000000"
                android:dashGap="8dp"
                android:dashWidth="2dp" />
        </shape>
    </item>

</layer-list>
